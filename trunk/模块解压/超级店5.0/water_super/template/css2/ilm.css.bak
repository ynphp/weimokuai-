.mui-checkbox .checkbox {
	position: absolute;
	top: -4px;
	left: 200px;
	width: 28px;
	height: 28px;
	-webkit-appearance: none;
	/* background-color: transparent; */
	border: 1px solid #D7D7D7;
	border-radius: 28px;
	outline: 0!important;
}
.mui-checkbox .box-active {
	font-size: 28px;
	font-weight: 400;
	line-height: 1;
	text-decoration: none;
	-webkit-font-smoothing: antialiased;
	background-color: #007aff;
	border: 1px solid #007aff;
}
.mui-checkbox .box-active:before {
	content: "\e472";
	color: #fff;
}
.mui-table-view li:first-child a.blue {
